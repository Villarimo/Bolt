async function startProgram() {
await scrollMatrixText('START', { r: 0, g: 255, b: 0 }, 30, true)
	await roll (0, 148, 1.2)
	await roll (0, 0, 1)
	setMainLed({ r: 0, g: 0, b: 255 })
	await spin (90, 1)
	await roll (90, 145, 0.7)
	await roll (90, 0, 1)
	await spin (180, 1)
	await roll (180, 143, 0.8)
	await roll (180, 0, 1)
	await Sound.Animal.play(true)
